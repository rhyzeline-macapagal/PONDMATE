<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- WiFi signal arcs with gradient strength -->
    <!-- Outermost arc (weakest signal) -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#E0E0E0"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:pathData="M2,13A10,10 0,0 1,22 13" />

    <!-- Second arc (medium signal) -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#BDBDBD"
        android:strokeWidth="2.5"
        android:strokeLineCap="round"
        android:pathData="M5,16A7,7 0,0 1,19 16" />

    <!-- Third arc (strong signal) -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#00448D"
        android:strokeWidth="3"
        android:strokeLineCap="round"
        android:pathData="M8,19A4,4 0,0 1,16 19" />

    <!-- Center dot (router/access point) -->
    <path
        android:fillColor="#00448D"
        android:pathData="M12,21m-1.5,0a1.5,1.5 0,1 1,3 0a1.5,1.5 0,1 1,-3 0" />

</vector>