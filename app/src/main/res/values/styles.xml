<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CircularImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="CustomDatePickerTheme" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="colorAccent">#B17457</item>
        <item name="android:textColorPrimary">#4A4947</item>
        <item name="android:headerBackground">#B17457</item>
        <item name="android:background">#D8D2C2</item>
    </style>

    <style name="CellStyleHeader">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/cell_border</item>
        <item name="android:padding">8dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#012E43</item>
        <item name="android:gravity">center</item>
    </style>

</resources>