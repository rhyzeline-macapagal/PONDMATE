<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CircularImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="CustomDatePickerTheme" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="colorAccent">#00448D</item>
        <item name="android:textColorPrimary">#01082D</item>
        <item name="android:headerBackground">#001C4F</item>
        <item name="android:background">#99B9D7</item>
    </style>

    <style name="CellStyleHeader">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/cell_border</item>
        <item name="android:padding">8dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#012E43</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TransparentDialog" parent="Theme.MaterialComponents.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>


</resources>